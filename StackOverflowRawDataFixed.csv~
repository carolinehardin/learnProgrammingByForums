class Program {
    static void Main(string[] args) {
        HostFactory.Run(x =&gt; {

            x.Service&lt;FooBar&gt;(s =&gt; {
                s.ConstructUsing(name =&gt; new OrderService());
                s.WhenStarted(os =&gt; os.Start());
                s.WhenStopped(os =&gt; os.Stop());
