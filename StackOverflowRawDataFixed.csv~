<pre><code>class S 

  def initialize(theX, theY)
    @x = theX + 2
    @y = theY + 3
  end

  def f(n)
    @x = n * @x + @y
    @y = @x + (n + 1) * @y
  end

  def to_s
    return (@y + 1).to_s + "" "" +
      (@x + 2).to_s  
  end
end

class T &lt; S
  def initialize
    super(1, 4)
    @z = @x + @y + 3
  end

  def f(m)
    super(3)
    @z *= m   
  end

  def to_s
    return super + "" "" + 
      (@z + 3).to_s
  end
end

s = S.new(6, 5)
s.f(3)
print s, ""\n""
t = T.new
t.f(7)
